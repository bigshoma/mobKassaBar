  <attributes>
    <name>СписокЗаказов</name>
    <title>
      <key>ru</key>
      <value>Список заказов</value>
    </title>
    <id>11</id>
    <valueType>
      <types>DynamicList</types>
    </valueType>
    <view>
      <common>true</common>
    </view>
    <edit>
      <common>true</common>
    </edit>
    <notDefaultUseAlwaysAttributes xsi:type="form:MultiLanguageDataPath">
      <paths xsi:type="form:DataPath">
        <segments>СписокЗаказов.ТелефонКлиента</segments>
      </paths>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:MultiLanguageDataPath">
      <paths xsi:type="form:DataPath">
        <segments>СписокЗаказов.ФИОКлиента</segments>
      </paths>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>СписокЗаказов.Date</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>СписокЗаказов.Ref</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>СписокЗаказов.ЖелаемаяДатаПокупки</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>СписокЗаказов.СортировкаПоСтатусу</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>СписокЗаказов.СтатусЗаказа</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>СписокЗаказов.СуммаДокумента</segments>
    </notDefaultUseAlwaysAttributes>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	ЗаказКлиента.Ссылка,
	ЗаказКлиента.Номер,
	ЗаказКлиента.Дата,
	ЗаказКлиента.ЖелаемаяДатаПокупки КАК ЖелаемаяДатаПокупки,
	ЗаказКлиента.НомерЗаказа,
	ЗаказКлиента.СтатусЗаказа,
	ЗаказКлиента.СуммаДокумента,
	ВЫБОР
		КОГДА ЗаказКлиента.СтатусЗаказа = ЗНАЧЕНИЕ(ПЕРЕЧИСЛЕНИЕ.СтатусыЗаказовКлиентов.НеСогласован)
			ТОГДА 0
		ИНАЧЕ ВЫБОР
				КОГДА ЗаказКлиента.СтатусЗаказа = ЗНАЧЕНИЕ(ПЕРЕЧИСЛЕНИЕ.СтатусыЗаказовКлиентов.Согласован)
					ТОГДА 1
				ИНАЧЕ ВЫБОР
						КОГДА ЗаказКлиента.СтатусЗаказа = ЗНАЧЕНИЕ(ПЕРЕЧИСЛЕНИЕ.СтатусыЗаказовКлиентов.Выполнен)
							ТОГДА 2
						ИНАЧЕ ВЫБОР
								КОГДА ЗаказКлиента.СтатусЗаказа = ЗНАЧЕНИЕ(ПЕРЕЧИСЛЕНИЕ.СтатусыЗаказовКлиентов.Отменен)
									ТОГДА 3
								ИНАЧЕ 99
							КОНЕЦ
					КОНЕЦ
			КОНЕЦ
	КОНЕЦ КАК СортировкаПоСтатусу,
	ЗаказКлиента.ФИО,
	ЗаказКлиента.Телефон
ИЗ
	Документ.ЗаказКлиента КАК ЗаказКлиента
ГДЕ
	ВЫБОР
			КОГДА &amp;ФильтрНаСегодня = ИСТИНА
				ТОГДА ЗаказКлиента.ЖелаемаяДатаПокупки &lt;= &amp;ТекущаяДата
						И ВЫБОР
							КОГДА НЕ ЗаказКлиента.ЖелаемаяДатаПокупки = &amp;ТекущаяДата
								ТОГДА ЗаказКлиента.СтатусЗаказа В (ЗНАЧЕНИЕ(ПЕРЕЧИСЛЕНИЕ.СтатусыЗаказовКлиентов.НеСогласован), ЗНАЧЕНИЕ(ПЕРЕЧИСЛЕНИЕ.СтатусыЗаказовКлиентов.Согласован))
							ИНАЧЕ ИСТИНА
						КОНЕЦ
			ИНАЧЕ ВЫБОР
					КОГДА &amp;ФильтрНаЗавтра = ИСТИНА
						ТОГДА ЗаказКлиента.ЖелаемаяДатаПокупки = ДОБАВИТЬКДАТЕ(&amp;ТекущаяДата, ДЕНЬ, 1)
					ИНАЧЕ ИСТИНА
				КОНЕЦ
		КОНЕЦ</queryText>
      <mainTable>Document.ЗаказКлиента</mainTable>
      <dynamicDataRead>true</dynamicDataRead>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
    </extInfo>
